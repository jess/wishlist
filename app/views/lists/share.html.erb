<h1><%= @list.user.name %></h1>
<h2>
  <%= @list.name %>
</h2>

<p>
  <b>Description:</b>
  <%= @list.description %>
</p>
<table class="items">
  <h2>Wish List</h2>
  <% @list.items.each do |item| %>
    <tr class="item">
      <td><%= item.name %></td>
      <td><%= link_to item.url, item.url, :target => "_blank" %></td>
    </tr>
  <% end %>
</table>
<div class="create_your_list">
  <h3>Create Your List</h3>
  <%= link_to "Create List", "http://apps.facebook.com/wishlistforall" %>
</div>
